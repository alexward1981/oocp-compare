// Icon fonts (imported from Fontastic)
@charset "UTF-8";

@font-face {
  font-family: "oocp-compare";
  src:url("/static/fonts/oocp-compare.eot");
  src:url("/static/fonts/oocp-compare.eot?#iefix") format("embedded-opentype"),
    url("/static/fonts/oocp-compare.woff") format("woff"),
    url("/static/fonts/oocp-compare.ttf") format("truetype"),
    url("/static/fonts/oocp-compare.svg#oocp-compare") format("svg");
  font-weight: normal;
  font-style: normal;

}

[data-icon]:before {
  font-family: "oocp-compare" !important;
  content: attr(data-icon);
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

[class^="icon-"]:before,
[class*=" icon-"]:before {
  font-family: "oocp-compare" !important;
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-court:before {
  content: "\63";
}
.icon-mediator:before {
  content: "\6d";
}
.icon-parent:before {
  content: "\70";
}
.icon-lawyer:before {
  content: "\6c";
}
.icon-cross:before {
  content: "\6e";
}
.icon-tick:before {
  content: "\79";
}
